<div class="container">
    <div id="card-container" class="row row-cols-1 row-cols-md-2 row-cols-lg-4 g-4">
            <% for(var j = cIndex; j < eIndex; j++) { %>
                <%- include('../partials/productCard', {img: imgPath + pgList[j]});%>
            <% } %> 
    </div>
</div>
        
<nav aria-label="...">
    <ul class="pagination">
        <li id="previous" class="page-item disabled">
            <button class="page-link" onclick="goToPreviousPage()">Previous</button>
        </li>
        <li class="page-item active">
            <button type="button" class="page-link" disabled><%= j %></button>
        </li>
        <% for(var j = 1; j <= pages; j++) {  %>
            <li class="page-item">
                <button class="page-link"><%= j %></button>
            </li>
        <% } %> 
        <form id="next-form" action="/next" method="POST">
            <input type="hidden" name="category" value="footwear">
            <li id="next" class="page-item">
                <button class="page-link" onclick="goToNextPage()">Next</button>
            </li>
        </form>
    </ul>
    </nav>